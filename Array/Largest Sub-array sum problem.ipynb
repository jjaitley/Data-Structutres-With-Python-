   "source": [
    "def max_subarray(a):\n",
    "    max_sub_array=current_count=a[0]\n",
    "    \n",
    "    for num in a[1:]:\n",
    "        current_count = max(current_count + num,num)\n",
    "        max_sub_array = max(current_count,max_sub_array)\n",
    "        \n",
    "    return max_sub_array"
   ]
